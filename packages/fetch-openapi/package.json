{
  "name": "fetch-openapi",
  "version": "0.6.1",
  "description": "Client SDK generator for openapi documents using fetch.",
  "main": "index.js",
  "bin": {
    "fetch-openapi": "./bin/fetch-openapi.js"
  },
  "scripts": {
    "cover": "istanbul cover _mocha -- ./test/*.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls -v",
    "test-watch": "mocha -w ./test/*.js",
    "test": "mocha ./test/*.js",
    "travis-test": "npm run cover && npm run coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kogosoftwarellc/open-api.git"
  },
  "keywords": [
    "fetch",
    "openapi",
    "generator",
    "sdk"
  ],
  "author": "Kogo Software LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kogosoftwarellc/open-api/issues"
  },
  "homepage": "https://github.com/kogosoftwarellc/open-api/tree/master/packages/fetch-openapi#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "glob": "^7.0.3",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "composites": "^1.1.1",
    "err-handler": "^1.0.5",
    "request": "*"
  }
}
