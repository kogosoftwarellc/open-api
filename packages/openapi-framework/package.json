{
  "name": "openapi-framework",
  "version": "0.1.0",
  "description": "An OpenAPI framework for any web framework",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "cover": "istanbul cover _mocha -- -r ts-node/register ./test/*.ts",
    "prepublishOnly": "tsc",
    "test-watch": "npm run test -- --watch-extensions ts -w",
    "test": "mocha -r ts-node/register ./index.ts ./test/*.ts ./test/**/*.ts",
    "travis-test": "npm run cover"
  },
  "repository": "https://github.com/kogosoftwarellc/open-api/tree/master/packages/openapi-framework",
  "keywords": [
    "openapi",
    "swagger",
    "framework"
  ],
  "author": "Joseph Spencer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kogosoftwarellc/open-api/issues?utf8=%E2%9C%93&q=is%3Aissue+is%3Aopen+label%3Aopenapi-framework"
  },
  "homepage": "https://github.com/kogosoftwarellc/open-api/tree/master/packages/openapi-framework#readme",
  "dependencies": {
    "difunc": "0.0.4",
    "fs-routes": "1.3.0",
    "glob": "*",
    "is-dir": "^1.0.0",
    "js-yaml": "^3.10.0",
    "openapi-default-setter": "1.0.0",
    "openapi-request-coercer": "1.0.0",
    "openapi-request-validator": "1.0.2",
    "openapi-response-validator": "1.0.2",
    "openapi-schema-validator": "1.0.0",
    "openapi-security-handler": "1.0.0",
    "openapi-types": "^1.0.4"
  }
}
